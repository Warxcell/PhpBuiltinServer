{
    "name": "arxy/codecept-phpbuiltinserver",
    "description": "PhpBuiltinServer extension for Codeception",
    "keywords": [
        "codeception"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Warxcell",
            "email": "warxcell@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "codeception/codeception": "^3.0 || ^4.0",
        "symfony/process": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Arxy\\Codecept\\PhpBuiltinServer\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
